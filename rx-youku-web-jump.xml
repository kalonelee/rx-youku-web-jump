<prescription xmlns="http://greenify.github.io/schemas/prescription/v3" type="activity" sender="other">
  <intent-filter>
    <action name="android.intent.action.VIEW" />
    <scheme name="market" />
    <ssp prefix="//details?id=com.youku.phone" />
  </intent-filter>
  <intent-filter>
    <action name="android.intent.action.VIEW" />
    <scheme name="http" />
    <ssp sglob="//market.android.com/.*com.youku.phone.*" />
  </intent-filter>
</prescription>